{% extends "base.djhtml" %}

{% block title %}
    Test Page
{% endblock %}

{% block content1 %}
  <h1> {{ person.firstName }} and {{ person.age }} </h1>
{% endblock %}

{% block content2 %}
    {% if x < 0 %}
        <h2> x is Negative </h2>
    {% elif x == 0 %}
        <h2> x is Zero </h2>
    {% else %}
        <h2> x is Positive </h2>
    {% endif %}

    <ul>
        {% for thing in stuff %}
            <li> {{ thing }} </li>
        {% endfor %}
    </ul>
{% endblock %}

{% block content3 %}
    <h1 id="post_out">No Post Recieved Yet</h1>
    <form method="post">
        {% csrf_token %}
        <label for="fname">Name</label><br>
        <input type="text" required="" name="fname" maxlength="150" id="form_id" autofocus="">
        <button type="submit">Clear</button>
    </form>
{% endblock %}

{% block script %}
    {% load static %}
    <script>
     $.ajaxSetup({
         headers: { "X-CSRFToken": '{{csrf_token}}' }
     });
    </script>
    <script src="{% static 'test.js' %}"></script>
{% endblock %}
